<?xml version='1.0'?>


<robot name="my_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">
    
    <xacro:include filename="$(find my_robot)/urdf/MSE6.gazebo" />

    <link name="robot_footprint"></link>

    <joint name="robot_footprint_joint" type="fixed">
        <origin xyz="0 0 0" rpy="0 0 0" />
        <parent link="robot_footprint"/>
	    <child link="chassis"/>
    </joint>

    <link name="chassis">
        <pose>0 0 0 0 0 0</pose>

        <inertial>
            <mass value="5.0"/>
            <origin xyz="0 0 .01" rpy="0 0 0"/>
            <inertia
                ixx="0.0112" ixy="0" ixz="0"
                iyy="0.0445" iyz="0"
                izz="0.0417"
                />
        </inertial>

        <collision name="collision">
            <origin xyz="0 0 .01" rpy="0 0 0"/>
            <geometry>
               <box size=".30 .1 .13"/> 
            </geometry>
        </collision>

        <visual name="chassis_visual">
            <origin xyz="0 0 -.05" rpy="0 0 0 "/>
            <geometry>
                <mesh filename="package://my_robot/meshes/MS6E.dae"/>
            </geometry>
        </visual>

    </link>
    <link name="right_wheel">

        <collision>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <geometry>
                <cylinder radius="0.025" length="0.015"/>
            </geometry>
            <surface>
                <friction>
                    <ode>
                        <mu>1</mu>
                        <mu2>0.01</mu2>
                    </ode>
                </friction>
            </surface>
        </collision>
        
        <inertial>
            <mass value=".1"/>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <inertia
            ixx="0.1" ixy="0" ixz="0"
            iyy="0.1" iyz="0"
            izz="0.1" 
            />
        </inertial>

        <visual>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <geometry>
                <cylinder radius="0.025" length="0.015"/>
            </geometry>
        </visual>
    
    </link>

    <link name="rr_wheel">

        <collision>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <geometry>
                <cylinder radius="0.030" length="0.015"/>
            </geometry>
            <surface>
                <friction>
                    <ode>
                        <mu>1</mu>
                        <mu2>0.01</mu2>
                    </ode>
                </friction>
            </surface>
        </collision>
        
        <inertial>
            <mass value=".1"/>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <inertia
            ixx="0.1" ixy="0" ixz="0"
            iyy="0.1" iyz="0"
            izz="0.1" 
            />
        </inertial>

        <visual>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <geometry>
                <cylinder radius="0.030" length="0.015"/>
            </geometry>
        </visual>
    
    </link>
    <link name="left_wheel">

        <collision>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <geometry>
                <cylinder radius="0.025" length="0.015"/>
            </geometry>
            <surface>
                <friction>
                    <ode>
                        <mu>1</mu>
                        <mu2>0.01</mu2>
                    </ode>
                </friction>
            </surface>
        </collision>
        
        <inertial>
            <mass value=".1"/>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <inertia
            ixx="0.1" ixy="0" ixz="0"
            iyy="0.1" iyz="0"
            izz="0.1" 
            />
        </inertial>

        <visual>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <geometry>
                <cylinder radius="0.025" length="0.015"/>
            </geometry>
        </visual>

    </link>
    <link name="lr_wheel">

        <collision>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <geometry>
                <cylinder radius="0.030" length="0.015"/>
            </geometry>
            <surface>
                <friction>
                    <ode>
                        <mu>1</mu>
                        <mu2>0.01</mu2>
                    </ode>
                </friction>
            </surface>
        </collision>
        
        <inertial>
            <mass value=".1"/>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <inertia
            ixx="0.1" ixy="0" ixz="0"
            iyy="0.1" iyz="0"
            izz="0.1" 
            />
        </inertial>

        <visual>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <geometry>
                <cylinder radius="0.030" length="0.015"/>
            </geometry>
        </visual>
    
    </link>
        <gazebo reference="left_wheel">
            <material>Gazebo/Black</material>
        </gazebo>
        
        <gazebo reference="right_wheel">
            <material>Gazebo/Black</material>
        </gazebo>
        
        <gazebo reference="lr_wheel">
            <material>Gazebo/Black</material>
        </gazebo>
        
        <gazebo reference="rr_wheel">
            <material>Gazebo/Black</material>
        </gazebo>
        
        <gazebo reference="chassis">
            <material>Gazebo/Road</material>
        </gazebo>

        <gazebo reference="camera">
            <material>Gazebo/Blue</material>
        </gazebo>


    <joint name="left_wheel_hinge" type="continuous">
        <origin xyz="0.09 0.067 -.05" rpy="0 0 0"/>
        <child link="left_wheel"/>
        <parent link="chassis"/>
        <axis xyz="0 1 0" rpy="0 0 0"/>
        <limit effort="100" velocity="100"/>
        <dynamics damping=".0" friction=".00"/>
    </joint>

    <joint name="right_wheel_hinge" type="continuous">
        <origin xyz="0.09 -0.067 -.05" rpy="0 0 0"/>
        <child link="right_wheel"/>
        <parent link="chassis"/>
        <axis xyz="0 1 0" rpy="0 0 0"/>
        <limit effort="100" velocity="100"/>
        <dynamics damping=".0" friction=".00"/>
    </joint>

    <joint name="lr_wheel_hinge" type="continuous">
        <origin xyz="-0.09 0.067 -.06" rpy="0 0 0"/>
        <child link="lr_wheel"/>
        <parent link="chassis"/>
        <axis xyz="0 1 0" rpy="0 0 0"/>
        <limit effort="100" velocity="100"/>
        <dynamics damping=".0" friction=".00"/>
    </joint>

    <joint name="rr_wheel_hinge" type="continuous">
        <origin xyz="-0.09 -0.067 -.06" rpy="0 0 0"/>
        <child link="rr_wheel"/>
        <parent link="chassis"/>
        <axis xyz="0 1 0" rpy="0 0 0"/>
        <limit effort="100" velocity="100"/>
        <dynamics damping=".0" friction=".00"/>
    </joint>
        
    <link name="camera">
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.02"/>
            </geometry>
        </collision>

        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.1"/>
            <inertia
                ixx="1e-6" ixy="0" ixz="0"
                iyy="1e-6" iyz="0"
                izz="1e-6"
                />
        </inertial>

        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.02"/>
            </geometry>
        </visual>

    </link>

    <joint name="camera_joint" type="fixed">
        <origin xyz="0.14 0 0.02" rpy="0 0 0"/>
        <parent link="chassis"/>
        <child link="camera"/>
        <axis xyz="0 1 0"/>
    </joint>

    <link name="hokuyo">
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.1 0.1 0.1"/>
            </geometry>
        </collision>

        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1e-5"/>
            <inertia 
                ixx="1e-6" ixy="0" ixz="0"
                iyy="1e-6" iyz="0"
                izz="1e-6"
                />
        </inertial>

        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://my_robot/meshes/hokuyo.dae"/>
            </geometry>
        </visual>

    </link>

    <joint name="hokuyo_joint" type="fixed">
        <origin xyz="0.03 0 .14" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
        <parent link="chassis"/>
        <child link="hokuyo"/>
    </joint>






</robot>



